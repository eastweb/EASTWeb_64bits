<?xml version="1.0"?>
<PluginMetadata>
	<Title>ModisLST</Title>
	<DaysPerInputData>8</DaysPerInputData>
	<Resolution>1000</Resolution>
	<ExtraDownloadFiles/>
	<Download>
		<DownloadFactoryClassName>ModisLSTFactory</DownloadFactoryClassName>
		<Mode>http</Mode>
		<HTTP>
			<URL>http://e4ftl01.cr.usgs.gov/MOLT/MOD11A2.005/</URL>
		</HTTP>
		<TimeZone>CST6CDT</TimeZone>
		<FilesPerDay>322</FilesPerDay>
		<DatePattern>\d{4}</DatePattern>
		<FileNamePattern>MOD11A2.A(\d{7}).h(\d{2})v(\d{2}).005.(\d{13}).hdf</FileNamePattern>
		<OriginDate>
			<DayOfMonth>05</DayOfMonth>
			<Month>March</Month>
			<Year>2000</Year>
		</OriginDate>
	</Download>
	<Processor>
		<ProcessStep>ModisLSTMozaic_HDF4</ProcessStep>
		<ProcessStep>ModisLSTFilter</ProcessStep>
		<ProcessStep>ModisLSTReproject</ProcessStep>
		<ProcessStep>ModisLSTMask</ProcessStep>
		<ProcessStep>ModisLSTClip</ProcessStep>
		<NumberOfOutput>2</NumberOfOutput>
	</Processor>
	<Indices>
		<ClassName>ModisLSTDay</ClassName>
		<ClassName>ModisLSTNight</ClassName>
		<ClassName>ModisLSTMean</ClassName>
	</Indices>
	<Summary>
		<Temporal>
			<MergeStrategyClass>AvgGdalRasterFileMerge</MergeStrategyClass>
			<InterpolateStrategyClass></InterpolateStrategyClass>
		</Temporal>
	</Summary>
	<QualityControl>
		<Level>HIGHEST</Level>
		<Level>MODERATE</Level>
		<Level>LOW</Level>
		<Level>NOSCREENING</Level>
	</QualityControl>
	<ExtraInfo>
		<Tiles>true</Tiles>
	</ExtraInfo>
</PluginMetadata>
