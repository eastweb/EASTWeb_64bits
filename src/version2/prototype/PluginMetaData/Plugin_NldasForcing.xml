<?xml version="1.0"?>
<PluginMetadata>
	<title>NldasForcing</title>
	<DaysPerInputData>1</DaysPerInputData>
	<ExtraDownloadFiles/>
	<Download>
		<className>NldasForcingDownloadTask</className>
		<mode>ftp</mode>
		<FTP>
			<hostName>hydro1.sci.gsfc.nasa.gov</hostName>
			<rootDir>/data/s4pa/NLDAS/NLDAS_FORA0125_H.002/</rootDir>
			<userName>anonymous</userName>
			<passWord>anonymous</passWord>
		</FTP>
		<TimeZone>(GMT+1:00) Africa/Bangui</TimeZone>
		<FilesPerDay>24</FilesPerDay>
		<datePattern>\\d{4}</datePattern>
		<fileNamePattern>NLDAS_FORA0125_H\\.A(\\d{4})(\\d{4})(\\d{4}).(\\d{4})00\\.002\\.grb</fileNamePattern>
		<OriginDate>
			<Day>01</Day>
			<Month>03</Month>
			<Year>2000</Year>
		</OriginDate>
	</Download>	
	<Processes>
		<className>NldasForcingComposite</className>
		<className>NldasForcingReproject</className>
		<className>NldasForcingClip</className>
		<className>NldasForcingMask</className>
	</Processes>
	<Indices>
		<ClassName></ClassName>
	</Indices>
	<Summary>
		<Temporal>
			<MergeStrategyClass>AvgGdalRasterFileMerge</MergeStrategyClass>
			<InterpolateStrategyClass/>
		</Temporal>
	</Summary>
	<QualityControl/>
</PluginMetadata>